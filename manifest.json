{
  "manifest_version": 3,
  "name": "GCal Guest Reminder",
  "version": "0.0.0.1",
  "description": "Tool to remind you to add guests on your google calendar meetings. Inspired by us repeatedly forgetting to invite guests...",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "css": ["css/inject.css"],
      "js": ["scripts/content.js"],
      "matches": [
        "https://calendar.google.com/*"
      ]
    }
  ]
}
